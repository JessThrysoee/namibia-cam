{
  "name": "namibia-cam",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "onFail": "error"
    }
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-player": "3.3.3",
    "react-rnd": "10.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.0",
    "@rsbuild/core": "1.5.17",
    "@rsbuild/plugin-react": "1.4.1",
    "@rsbuild/plugin-type-check": "1.2.4",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "npm-run-all2": "8.0.4",
    "typescript": "5.9.3"
  },
  "scripts": {
    "start": "run-s dev",
    "build": "rsbuild build",
    "check": "biome check --write",
    "dev": "rsbuild dev --open",
    "preview": "rsbuild preview",
    "format": "biome format --write",
    "lint:check": "biome check --error-on-warnings",
    "lint": "run-s lint:check",
    "npm-check-updates": "npx --yes npm-check-updates@latest -i --format group"
  }
}
